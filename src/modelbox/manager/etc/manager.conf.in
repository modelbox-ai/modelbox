# log level
loglevel INFO

# log file path
logfile /var/log/modelbox/manager.log

#
# app -name "appname" -pidfile "/run/app.pid" -keep-check -- run command list here
#
app -name "modelbox" -check-alive -pidfile "/@CMAKE_INSTALL_RUNSTATEDIR@/modelbox/modelbox.pid" -- @CMAKE_INSTALL_FULL_BINDIR@/modelbox -c @CMAKE_INSTALL_FULL_SYSCONFDIR@/modelbox/modelbox.conf -p /@CMAKE_INSTALL_RUNSTATEDIR@/modelbox/modelbox.pid -n modelbox

