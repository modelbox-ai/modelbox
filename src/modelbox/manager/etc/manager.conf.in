# log level
loglevel INFO

# log file path
logfile @MODELBOX_ROOT_VAR@/var/log/modelbox/manager.log

#
# app -name "appname" -pidfile "@MODELBOX_ROOT_VAR@/run/app.pid" -keep-check -- run command list here
#
app -name "modelbox" -pidfile "@MODELBOX_ROOT_VAR@/@CMAKE_INSTALL_RUNSTATEDIR@/modelbox/modelbox.pid" -- @MODELBOX_ROOT_VAR@/etc/init.d/modelbox start

