[base]
name = "inference_plugin"
device = "cuda"
version = "1.1.2"
description = "a cuda inference flowunit"
entry = "@CMAKE_SOURCE_DIR@/test/assets/tensorflow/TF_VERSION/tensorflow_pb/frozen_model.pb"
type = "inference"
virtual_type = "tensorflow"

[config]
plugin = "@CMAKE_SOURCE_DIR@/build/test/test-working-dir/drivers/libmodelbox-unit-inferece-plugin.so"

[input]
[input.input1]
name = "input"
type = "float"
device = "cpu"

[output]
[output.output1]
name = "output"
type = "float"